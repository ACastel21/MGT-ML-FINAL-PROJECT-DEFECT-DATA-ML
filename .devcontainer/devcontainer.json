{
    "name": "MGT665 ML Project",
    "image": "mcr.microsoft.com/devcontainers/python:3.0",
    "features": {
      "ghcr.io/devcontainers/features/python:1": {
        "version": "3.0"
      }
    },
    "postCreateCommand": "pip install -r requirements.txt",
    "customizations": {
      "vscode": {
        "extensions": [
          "ms-python.python",
          "ms-toolsai.jupyter",
          "ms-python.vscode-pylance",
          "github.copilot",
          "donjayamanne.python-environment-manager"
        ]
      }
    }
  }
  